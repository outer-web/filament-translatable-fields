{
  "name": "outerweb/filament-translatable-fields",
  "description": "This package adds a way to interact with outerweb/translatable-fields in Filament.",
  "homepage": "https://github.com/outer-web/filament-translatable-fields",
  "license": "MIT",
  "authors": [
    {
      "name": "Outerweb",
      "email": "info@outerweb.be"
    }
  ],
  "require": {
    "php": "^8.1",
    "filament/filament": "^4.0",
    "laravel/framework": "^10.0|^11.0|^12.0",
    "spatie/laravel-package-tools": "^1.16",
    "spatie/laravel-translatable": "^6.6"
  },
  "autoload": {
    "psr-4": {
      "Outerweb\\FilamentTranslatableFields\\": "src/"
    },
    "files": [
      "src\\Helpers\\helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Outerweb\\FilamentTranslatableFields\\FilamentTranslatableFieldsServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
